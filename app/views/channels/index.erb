<h1>Current Available Channels</h1>
<ul>
  <% @all_channels.each do |channel| %>
    <li> <a href="/channels/<%= channel.id %>"> <%= channel.name %></a> </li>
  <% end %>
</ul>
<div>
<% if logged_in? %>
  <a href='/users/<%= current_user.id %>'>Home</a>
<% else %>
  <a href='/'>Home</a>
<% end %>
</div>
